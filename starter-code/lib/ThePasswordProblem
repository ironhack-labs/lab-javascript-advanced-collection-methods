
//Iteration3.1

var goodPsswd = "1234567890";
var badPsswd = "1123456";


var noRepeatChar = function (password) {
 //your code goes here
 var a = password.split("");
 var b= _.uniq(a);



if (b.length < goodPsswd.length)
{
   return ("error");

}
else

   {return("right password")}


};

noRepeatChar(goodPsswd);
noRepeatChar(badPsswd);
